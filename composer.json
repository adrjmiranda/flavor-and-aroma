{
	"name": "adriano/flavor-and-aroma",
	"description": "Culinary Recipes Blog is a web application developed in pure PHP. This project aims to showcase my web development skills and serve as a portfolio example.",
	"type": "project",
	"license": "MIT License",
	"scripts": {
		"serve": "php -S localhost:8000 -t public"
	},
	"autoload": {
		"psr-4": {
			"App\\": "App/"
		}
	},
	"authors": [
		{
			"name": "Adriano Miranda",
			"email": "adrjmiranda@gmail.com"
		}
	],
	"require": {
		"vlucas/phpdotenv": "^5.6",
		"slim/slim": "4.*",
		"slim/psr7": "^1.7",
		"twig/twig": "^3.0",
		"firebase/php-jwt": "^6.10",
		"doctrine/orm": "^3.2",
		"doctrine/dbal": "^4.0",
		"symfony/cache": "^7.1",
		"doctrine/migrations": "^3.8"
	},
	"config": {
		"process-timeout": 0
	},
	"require-dev": {
		"symfony/var-dumper": "^7.1"
